  "version": "1.0.0",
  "supportsMobile": true,
  "name": "RiskGauge",
  "newInstancePrefix": "RiskGauge",
  "description": "Risk Level Gauge (SVG) \u2014 Neo-ready",
  "webcomponents": [
    {
      "kind": "main",
      "tag": "com-sap-sac-sample-echarts-gaugegrade",
