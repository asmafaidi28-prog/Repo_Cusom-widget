      "integrity": "",
      "ignoreIntegrity": true
    },
    {
      "kind": "styling",
      "tag": "com-sap-sac-sample-echarts-gaugegrade-styling",
      "url": "https://<your-username>.github.io/<your-repo>/sac-risk-gauge/styling.js",
      "integrity": "",
      "ignoreIntegrity": true
    }
  ],
  "properties": {
    "value": {
      "type": "number",
      "default": 50
    },
    "stops": {
      "type": "number[]",
      "default": [
        0,
        25,
        50,
        75,
        100
      ]
    },
    "reverse": {
      "type": "boolean",
      "default": false
    }
  },
  "events": {},
  "methods": {},
  "types": {
    "Range": {
      "description": "Threshold/color pair",
      "properties": {
        "value": {
          "type": "number",
          "description": "Stop value",
          "mandatory": true
        },
        "color": {
          "type": "string",
          "description": "Hex or rgba",
          "mandatory": true
        }
      }
    }
  },
  "dataBindings": {
    "dataBinding": {
      "name": "Data Binding",
      "feeds": [
        {
          "id": "dimensions",
          "description": "Dimensions",
          "type": "dimension",
          "min": 0,
          "max": 1
        },
        {
          "id": "measures",
          "description": "Measures",
          "type": "measure",
          "min": 1,
          "max": 1
        }
      ]
    }
  }
